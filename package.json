{
  "name": "postcss-calc",
  "version": "7.0.2",
  "description": "PostCSS plugin to reduce calc()",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "calculation",
    "calc"
  ],
  "main": "src/index.js",
  "files": [
    "src/lib",
    "src/index.js",
    "src/parser.js",
    "LICENSE"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "jison src/parser.jison -o src/parser.js",
    "pretest": "npm run build && eslint src",
    "test": "ava"
  },
  "author": "Andy Jansson",
  "license": "MIT",
  "repository": "https://github.com/postcss/postcss-calc.git",
  "eslintConfig": {
    "extends": "eslint-config-i-am-meticulous",
    "rules": {
      "curly": "error"
    }
  },
  "eslintIgnore": [
    "src/parser.js"
  ],
  "devDependencies": {
    "ava": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-i-am-meticulous": "^12.0.0",
    "jison-gho": "^0.6.1-215"
  },
  "dependencies": {
    "postcss": "^7.0.27",
    "postcss-selector-parser": "^6.0.2",
    "postcss-value-parser": "^4.0.2"
  }
}
